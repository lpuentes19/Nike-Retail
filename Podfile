# platform :ios, '11.3'

target 'Nike-Retail' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

	# Firebase
	pod 'Firebase/Core'
	pod 'Firebase/Auth'
	pod 'Firebase/Database'
 	pod 'Firebase/Storage'
	
	# Stripe
	pod 'Stripe'

	# Networking
	pod 'AFNetworking'

end
